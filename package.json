{
  "name": "scomap",
  "private": true,
  "packageManager": "pnpm@9.15.4",
  "scripts": {
    "dev": "turbo run dev",
    "build": "turbo run build",
    "lint": "turbo run lint",
    "db:generate": "turbo run db:generate --filter=@scomap/db",
    "db:migrate": "turbo run db:migrate --filter=@scomap/db",
    "db:studio": "turbo run db:studio --filter=@scomap/db"
  },
  "devDependencies": {
    "turbo": "^2.3.3",
    "typescript": "^5.7.3"
  },
  "engines": {
    "node": ">=20.0.0"
  }
}
